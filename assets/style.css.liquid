}



/* 5.2 ALERTS */
.alert {
	margin: 15px 0;
	-webkit-border-radius: 0;
	   -moz-border-radius: 0;
	        border-radius: 0;
	}

.alert > p,
.alert > ul,
.alert > ol {
	list-style-type: none;
	margin: 0;
}

.alert > p + p,
.alert > ul + ul {
	margin: 10px 0 0 0;
}


/* 5.3 BUTTONS */
.btn,
.btn a,
.btn-info,
.btn-primary {
	padding: 11px 20px 12px 20px;
	background: {{ settings.color_1 }} !important;
